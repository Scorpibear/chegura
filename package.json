{
  "name": "chegura",
  "version": "1.0.0",
  "scripts": {
    "start": "node app.js",
    "test": "jasmine --random=false",
    "tdd": "nodemon --exec jasmine --random=false",
    "lint": "standard",
    "coverage": "istanbul cover node_modules/jasmine/bin/jasmine.js",
    "validate": "npm-run-all --parallel test lint"
  },
  "dependencies": {
    "fen-analyzer": "^0.1.0",
    "uci-adapter": "github:Scorpibear/uci-adapter"
  },
  "description": "chegura is a chess-guru - server providing best chess moves",
  "main": "app.js",
  "devDependencies": {
    "eslint": "^4.19.0",
    "eslint-config-google": "^0.9.1",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "jasmine": "^3.1.0",
    "jshint": "^2.9.5",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.2",
    "standard": "^10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Scorpibear/chegura.git"
  },
  "keywords": [
    "chess",
    "server"
  ],
  "author": "Mikhail Subach",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Scorpibear/chegura/issues"
  },
  "homepage": "https://github.com/Scorpibear/chegura#readme",
  "nodemonConfig": {
    "watch": [
      "app/**",
      "spec/**",
      "app.js"
    ]
  }
